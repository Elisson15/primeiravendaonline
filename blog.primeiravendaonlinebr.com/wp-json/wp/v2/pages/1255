ground-color: #13DC01;\n            color: #000;\n            font-size: 16px;\n            font-weight: bold;\n            padding: 15px;\n            border: none;\n            border-radius: 5px;\n            margin-top: 10px;\n            cursor: pointer;\n            transition: all 0.3s;\n            width: 100%;\n        }\n\n        .btn:hover {\n            background-color: #0eb301;\n        }\n\n        .result {\n            font-size: 18px;\n            margin-top: 20px;\n            font-weight: bold;\n        }\n\n        .correct {\n            color: #13DC01;\n        }\n\n        .incorrect {\n            color: #ff4d4d;\n        }\n\n        .hidden {\n            display: none;\n        }\n\n        \/* Responsividade para telas menores *\/\n        @media (max-width: 768px) {\n            body {\n                padding: 15px;\n            }\n            h1 {\n                font-size: 20px;\n            }\n            .question-container {\n                padding: 15px;\n            }\n            .btn {\n                font-size: 14px;\n                padding: 12px;\n            }\n        }\n\n        @media (max-width: 480px) {\n            h1 {\n                font-size: 18px;\n            }\n            .btn {\n                font-size: 14px;\n                padding: 12px;\n            }\n        }\n    <\/style>\n<\/head>\n<body>\n    <h1>Acerte a Resposta - Marketing Digital<\/h1>\n    <div class=\"question-container\" id=\"question-container\">\n        <p id=\"question\"><strong>Pergunta aqui...<\/strong><\/p>\n        <button class=\"btn\" onclick=\"checkAnswer(0)\" id=\"option0\">Op\u00e7\u00e3o 1<\/button>\n        <button class=\"btn\" onclick=\"checkAnswer(1)\" id=\"option1\">Op\u00e7\u00e3o 2<\/button>\n        <button class=\"btn\" onclick=\"checkAnswer(2)\" id=\"option2\">Op\u00e7\u00e3o 3<\/button>\n        <p class=\"result\" id=\"result\"><\/p>\n        <button class=\"btn hidden\" id=\"next-btn\" onclick=\"nextQuestion()\">Pr\u00f3xima Pergunta<\/button>\n    <\/div>\n\n    <script>\n        \/\/ Lista de perguntas e respostas\n        const questions = [\n            {\n                question: \"O que \u00e9 um afiliado no Marketing Digital?\",\n                options: [\"Uma pessoa que cria e vende infoprodutos\", \"Uma pessoa que promove produtos de outros e ganha comiss\u00e3o\", \"Uma pessoa que compra produtos para revender\"],\n                answer: 1\n            },\n            {\n                question: \"O que \u00e9 tr\u00e1fego org\u00e2nico?\",\n                options: [\"Tr\u00e1fego pago atrav\u00e9s de an\u00fancios\", \"Tr\u00e1fego que vem de busca natural e intera\u00e7\u00f5es sem pagar\", \"Tr\u00e1fego gerado atrav\u00e9s de e-mails\"],\n                answer: 1\n            },\n            {\n                question: \"O que \u00e9 um funil de vendas?\",\n                options: [\"Uma forma de vender produtos em grande escala\", \"Uma estrat\u00e9gia para guiar o cliente do interesse at\u00e9 a compra\", \"Uma t\u00e9cnica de vendas utilizada apenas em lojas f\u00edsicas\"],\n                answer: 1\n            },\n            {\n                question: \"Qual dessas estrat\u00e9gias ajuda a gerar tr\u00e1fego org\u00e2nico?\",\n                options: [\"Investir em an\u00fancios pagos\", \"Criar conte\u00fados valiosos e interagir nas redes sociais\", \"Comprar seguidores para o perfil\"],\n                answer: 1\n            },\n            {\n                question: \"O que significa 'CTA' no marketing digital?\",\n                options: [\"Call to Action (Chamada para A\u00e7\u00e3o)\", \"Campanha de Tr\u00e1fego Avan\u00e7ado\", \"Custo Total de Aquisi\u00e7\u00e3o\"],\n                answer: 0\n            },\n            {\n                question: \"O que s\u00e3o infoprodutos?\",\n                options: [\"Produtos f\u00edsicos como livros e roupas\", \"Produtos digitais como e-books, cursos e podcasts\", \"Produtos que s\u00e3o vendidos exclusivamente em lojas virtuais\"],\n                answer: 1\n            },\n            {\n                question: \"O que \u00e9 importante para um afiliado gerar mais vendas?\",\n                options: [\"Ter muitos seguidores\", \"Criar conte\u00fado de valor para o p\u00fablico\", \"Vender apenas produtos caros\"],\n                answer: 1\n            },\n            {\n                question: \"Qual \u00e9 a vantagem do tr\u00e1fego org\u00e2nico?\",\n                options: [\"Ele \u00e9 gratuito e pode gerar resultados a longo prazo\", \"Ele \u00e9 mais r\u00e1pido que tr\u00e1fego pago\", \"Ele exige investimentos em an\u00fancios para ser eficaz\"],\n                answer: 0\n            },\n            {\n                question: \"Qual \u00e9 o primeiro passo ao criar um funil de vendas?\",\n                options: [\"Atra\u00e7\u00e3o de tr\u00e1fego qualificado\", \"Criar produtos caros\", \"Comprar an\u00fancios pagos\"],\n                answer: 0\n            },\n            {\n                question: \"Como voc\u00ea pode melhorar a taxa de convers\u00e3o de suas vendas?\",\n                options: [\"Fazer postagens di\u00e1rias sem se preocupar com a qualidade\", \"Oferecer algo de valor para seu p\u00fablico e ter uma boa comunica\u00e7\u00e3o\", \"Focar apenas em an\u00fancios pagos\"],\n                answer: 1\n            }\n        ];\n\n        let currentQuestionIndex = 0;\n        let score = 0;\n\n        \/\/ Embaralha as perguntas aleatoriamente\n        questions.sort(() => Math.random() - 0.5);\n\n        \/\/ Fun\u00e7\u00e3o para carregar a pergunta\n        function loadQuestion() {\n            let q = questions[currentQuestionIndex];\n            document.getElementById(\"question\").innerText = q.question;\n            document.getElementById(\"option0\").innerText = q.options[0];\n            document.getElementById(\"option1\").innerText = q.options[1];\n            document.getElementById(\"option2\").innerText = q.options[2];\n            document.getElementById(\"result\").innerText = \"\";\n            document.getElementById(\"next-btn\").classList.add(\"hidden\");\n        }\n\n        \/\/ Checa a resposta escolhida\n        function checkAnswer(selected) {\n            let q = questions[currentQuestionIndex];\n            if (selected === q.answer) {\n                document.getElementById(\"result\").innerHTML = \"\ud83c\udf89 Resposta correta!\";\n                document.getElementById(\"result\").className = \"result correct\";\n                score++;\n            } else {\n                document.getElementById(\"result\").innerHTML = \"\u274c Resposta errada!\";\n                document.getElementById(\"result\").className = \"result incorrect\";\n            }\n            document.getElementById(\"next-btn\").classList.remove(\"hidden\");\n\n            \/\/ Desabilita os bot\u00f5es de resposta ap\u00f3s sele\u00e7\u00e3o\n            document.getElementById(\"option0\").disabled = true;\n            document.getElementById(\"option1\").disabled = true;\n            document.getElementById(\"option2\").disabled = true;\n        }\n\n        \/\/ Pr\u00f3xima pergunta\n        function nextQuestion() {\n            currentQuestionIndex++;\n            if (currentQuestionIndex < questions.length) {\n                loadQuestion();\n                \/\/ Habilita os bot\u00f5es novamente\n                document.getElementById(\"option0\").disabled = false;\n                document.getElementById(\"option1\").disabled = false;\n                document.getElementById(\"option2\").disabled = false;\n            } else {\n                showFinalResult();\n            }\n        }\n\n        \/\/ Mostra o resultado final\n        function showFinalResult() {\n            let level = '';\n            let bonus = '';\n            if (score === questions.length) {\n                level = 'Avan\u00e7ado! Voc\u00ea realmente entende de marketing digital!';\n            } else if (score >= questions.length \/ 2) {\n                level = 'Intermedi\u00e1rio! Voc\u00ea est\u00e1 indo bem, continue assim!';\n            } else {\n                level = 'Iniciante! Vamos continuar aprendendo, voc\u00ea chega l\u00e1!';\n            }\n\n            if (score >= 8) {\n                bonus = `\n                    <p>\ud83c\udf81 Parab\u00e9ns! Voc\u00ea acertou 8 ou mais perguntas. Aqui est\u00e1 um b\u00f4nus exclusivo para voc\u00ea:<\/p>\n                    <a href=\"https:\/\/docs.google.com\/document\/d\/1B2tqETi2BhmIaH6XasGTe8J2UO43bvafJb_7-jWxjvc\/edit?tab=t.0\" class=\"btn\">Acesse seu B\u00f4nus!<\/a>\n                `;\n            }\n\n            document.getElementById(\"question-container\").innerHTML = `\n                <h2>Voc\u00ea acertou ${score} de ${questions.length} perguntas!<\/h2>\n                <p>${level}<\/p>\n                ${bonus}\n                <button class=\"btn\" onclick=\"restartQuiz()\">Tentar Novamente<\/button>\n            `;\n        }\n\n        \/\/ Reinicia o quiz com a atualiza\u00e7\u00e3o da p\u00e1gina\n        function restartQuiz() {\n            location.reload();  \/\/ Recarrega a p\u00e1gina\n        }\n\n        \/\/ Carrega a primeira pergunta ao iniciar\n        loadQuestion();\n    <\/script>\n<\/body>\n<\/html>\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"elementor-element elementor-element-eeb8791 e-flex e-con-boxed e-con e-parent\" data-id=\"eeb8791\" data-element_type=\"container\">\n\t\t\t\t\t<div class=\"e-con-inner\">\n\t\t\t\t<div class=\"elementor-element elementor-element-e8e1fde elementor-widget elementor-widget-html\" data-id=\"e8e1fde\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<!DOCTYPE html>\n<html lang=\"pt-br\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\