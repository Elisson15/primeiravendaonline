t>\n        \/\/ Lista de perguntas e respostas\n        const questions = [\n            {\n                question: \"O que \u00e9 tr\u00e1fego org\u00e2nico?\",\n                options: [\"Tr\u00e1fego pago por an\u00fancios\", \"Tr\u00e1fego que vem de pesquisa e intera\u00e7\u00f5es naturais\", \"Tr\u00e1fego de links patrocinados\"],\n                answer: 1\n            },\n            {\n                question: \"Como voc\u00ea pode aumentar seu tr\u00e1fego org\u00e2nico?\",\n                options: [\"Postando conte\u00fado frequentemente e utilizando SEO\", \"Gastando muito dinheiro em an\u00fancios\", \"Comprando seguidores\"],\n                answer: 0\n            },\n            {\n                question: \"Qual das op\u00e7\u00f5es abaixo \u00e9 uma estrat\u00e9gia de marketing para promover infoprodutos?\",\n                options: [\"Criar v\u00eddeos educacionais e publicar em redes sociais\", \"Usar apenas an\u00fancios pagos\", \"Ignorar a produ\u00e7\u00e3o de conte\u00fado\"],\n                answer: 0\n            },\n            {\n                question: \"Qual \u00e9 a principal vantagem de um funil de vendas?\",\n                options: [\"Gerar vendas instant\u00e2neas\", \"Capturar leads e nutrir relacionamentos com o cliente\", \"Criar um site para vender rapidamente\"],\n                answer: 1\n            },\n            {\n                question: \"O que s\u00e3o leads em marketing digital?\",\n                options: [\"Pessoas que j\u00e1 compraram um produto\", \"Pessoas interessadas em receber mais informa\u00e7\u00f5es\", \"Clientes finais que est\u00e3o prontos para comprar\"],\n                answer: 1\n            },\n            {\n                question: \"O que \u00e9 SEO (Otimiza\u00e7\u00e3o para Mecanismos de Busca)?\",\n                options: [\"A t\u00e9cnica de comprar tr\u00e1fego\", \"O processo de otimizar o conte\u00fado para aparecer nos motores de busca\", \"Criar an\u00fancios pagos para aparecer no Google\"],\n                answer: 1\n            },\n            {\n                question: \"Por que os infoprodutores devem investir em estrat\u00e9gias de conte\u00fado?\",\n                options: [\"Para educar seu p\u00fablico e gerar confian\u00e7a\", \"Para vender diretamente aos visitantes\", \"Para comprar tr\u00e1fego barato\"],\n                answer: 0\n            },\n            {\n                question: \"O que significa 'CTA' no marketing digital?\",\n                options: [\"Cliente T\u00e9cnico Autom\u00e1tico\", \"Chamada para A\u00e7\u00e3o\", \"Captura de Texto Avan\u00e7ada\"],\n                answer: 1\n            },\n            {\n                question: \"Como um blog pode ajudar a vender infoprodutos?\",\n                options: [\"Gerando tr\u00e1fego org\u00e2nico com conte\u00fado relevante\", \"Comprando tr\u00e1fego pago\", \"Ignorando as intera\u00e7\u00f5es com os leitores\"],\n                answer: 0\n            },\n            {\n                question: \"O que \u00e9 uma estrat\u00e9gia de email marketing?\",\n                options: [\"Enviar muitos e-mails sem segmenta\u00e7\u00e3o\", \"Enviar e-mails com ofertas personalizadas e segmentadas\", \"Enviar e-mails apenas para quem j\u00e1 comprou um produto\"],\n                answer: 1\n            }\n        ];\n\n        let currentQuestionIndex = 0;\n        let score = 0;\n\n        \/\/ Embaralha as perguntas aleatoriamente\n        questions.sort(() => Math.random() - 0.5);\n\n        \/\/ Fun\u00e7\u00e3o para carregar a pergunta\n        function loadQuestion() {\n            let q = questions[currentQuestionIndex];\n            document.getElementById(\"question\").innerText = q.question;\n            document.getElementById(\"option0\").innerText = q.options[0];\n            document.getElementById(\"option1\").innerText = q.options[1];\n            document.getElementById(\"option2\").innerText = q.options[2];\n            document.getElementById(\"result\").innerText = \"\";\n            document.getElementById(\"next-btn\").classList.add(\"hidden\");\n        }\n\n        \/\/ Checa a resposta escolhida\n        function checkAnswer(selected) {\n            let q = questions[currentQuestionIndex];\n            if (selected === q.answer) {\n                document.getElementById(\"result\").innerHTML = \"\ud83c\udf89 Resposta correta!\";\n                document.getElementById(\"result\").className = \"result correct\";\n                score++;\n            } else {\n                document.getElementById(\"result\").innerHTML = \"\u274c Resposta errada!\";\n                document.getElementById(\"result\").className = \"result incorrect\";\n            }\n            document.getElementById(\"next-btn\").classList.remove(\"hidden\");\n\n            \/\/ Desabilita os bot\u00f5es de resposta ap\u00f3s sele\u00e7\u00e3o\n            document.getElementById(\"option0\").disabled = true;\n            document.getElementById(\"option1\").disabled = true;\n            document.getElementById(\"option2\").disabled = true;\n        }\n\n        \/\/ Pr\u00f3xima pergunta\n        function nextQuestion() {\n            currentQuestionIndex++;\n            if (currentQuestionIndex < questions.length) {\n                loadQuestion();\n                \/\/ Habilita os bot\u00f5es novamente\n                document.getElementById(\"option0\").disabled = false;\n                document.getElementById(\"option1\").disabled = false;\n                document.getElementById(\"option2\").disabled = false;\n            } else {\n                showFinalResult();\n            }\n        }\n\n        \/\/ Mostra o resultado final\n        function showFinalResult() {\n            let level = '';\n            let bonus = '';\n            if (score === questions.length) {\n                level = 'Avan\u00e7ado! Voc\u00ea realmente entende de Marketing Digital!';\n            } else if (score >= questions.length \/ 2) {\n                level = 'Intermedi\u00e1rio! Voc\u00ea est\u00e1 indo bem, continue assim!';\n            } else {\n                level = 'Iniciante! Vamos continuar aprendendo, voc\u00ea chega l\u00e1!';\n            }\n\n            if (score >= 8) {\n                bonus = `\n                    <p>\ud83c\udf81 Parab\u00e9ns! Voc\u00ea acertou 8 ou mais perguntas. Aqui est\u00e1 um b\u00f4nus exclusivo para voc\u00ea:<\/p>\n                    <a href=\"https:\/\/drive.google.com\/drive\/u\/0\/folders\/1-DMJVPH7_Ng1iJIOL2ZS2LmhLN4m4DIV\" class=\"btn\">Acesse seu B\u00f4nus!<\/a>\n                `;\n            }\n\n            document.getElementById(\"question-container\").innerHTML = `\n                <h2>Voc\u00ea acertou ${score} de ${questions.length} perguntas!<\/h2>\n                <p>${level}<\/p>\n                ${bonus}\n                <button class=\"btn\" onclick=\"restartQuiz()\">Tentar Novamente<\/button>\n            `;\n        }\n\n        \/\/ Reinicia o quiz com a atualiza\u00e7\u00e3o da p\u00e1gina\n        function restartQuiz() {\n            location.reload();  \/\/ Recarrega a p\u00e1gina\n        }\n\n        \/\/ Carrega a primeira pergunta ao iniciar\n        loadQuestion();\n    <\/script>\n<\/body>\n<\/html>\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"elementor-element elementor-element-00f677a e-flex e-con-boxed e-con e-parent\" data-id=\"00f677a\" data-element_type=\"container\">\n\t\t\t\t\t<div class=\"e-con-inner\">\n\t\t\t\t<div class=\"elementor-element elementor-element-f1d365c elementor-widget elementor-widget-html\" data-id=\"f1d365c\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<!DOCTYPE html>\n<html lang=\"pt-br\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Voltar para P\u00e1gina In