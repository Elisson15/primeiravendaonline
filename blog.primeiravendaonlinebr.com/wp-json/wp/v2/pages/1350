ize: 18px;\n            font-weight: bold;\n            cursor: pointer;\n            text-transform: uppercase;\n        }\n        .found {\n            background-color: #13DC01;\n            color: white;\n        }\n        #word-list {\n            list-style: none;\n            padding: 0;\n        }\n        #word-list li {\n            font-size: 18px;\n            margin: 10px 0;\n            font-weight: bold;\n            color: black;\n        }\n        .description {\n            font-size: 16px;\n            font-weight: bold;\n            color: #13DC01;\n            display: none;\n        }\n        #message {\n            font-size: 22px;\n            font-weight: bold;\n            color: #13DC01;\n            margin-top: 20px;\n            display: none;\n        }\n        .coupon {\n            font-size: 20px;\n            font-weight: bold;\n            color: #13DC01;\n            margin-top: 15px;\n            display: none;\n        }\n        .coupon a {\n            text-decoration: none;\n            color: white;\n            background: #13DC01;\n            padding: 10px 20px;\n            border-radius: 5px;\n        }\n    <\/style>\n<\/head>\n<body>\n\n    <h1>Ca\u00e7a-Palavras de Marketing Digital<\/h1>\n    <div class=\"grid\" id=\"grid\"><\/div>\n    <h2>Encontre as palavras:<\/h2>\n    <ul id=\"word-list\"><\/ul>\n    <p id=\"message\">Parab\u00e9ns, voc\u00ea acertou todas as palavras!<\/p>\n    <div class=\"coupon\" id=\"coupon\">\n        \ud83c\udf89 Use o cupom <strong>COMUNIDADEPVO2025<\/strong> e ganhe 15% de desconto!<br>\n        <a href=\"https:\/\/pay.cakto.com.br\/4onaoYc\" target=\"_blank\">Aproveitar desconto<\/a>\n    <\/div>\n\n    <script>\n        const palavras = [\n            { word: \"org\u00e2nico\", desc: \"Quando voc\u00ea atrai p\u00fablico sem pagar por an\u00fancios.\" },\n            { word: \"stories\", desc: \"Conte\u00fados tempor\u00e1rios nas redes sociais.\" },\n            { word: \"copy\", desc: \"Texto persuasivo usado para vender produtos.\" },\n            { word: \"cta\", desc: \"Chamada para a\u00e7\u00e3o, como 'clique aqui'.\" },\n            { word: \"lead\", desc: \"Pessoa interessada no seu produto.\" },\n            { word: \"tr\u00e1fego\", desc: \"Visitantes que chegam ao seu site.\" },\n            { word: \"lan\u00e7ar\", desc: \"Divulgar um produto ao p\u00fablico.\" },\n            { word: \"autoridade\", desc: \"Ser refer\u00eancia no seu nicho.\" },\n            { word: \"branding\", desc: \"Constru\u00e7\u00e3o da identidade de uma marca.\" }\n        ];\n\n        let palavrasSelecionadas = [];\n        let palavrasEncontradas = 0;\n        const gridSize = 10;\n        let grid = Array.from({ length: gridSize }, () => Array(gridSize).fill(\"\"));\n\n        function selecionarPalavras() {\n            palavrasSelecionadas = palavras.sort(() => Math.random() - 0.5).slice(0, 5);\n        }\n\n        function colocarPalavra(word) {\n            let placed = false;\n            while (!placed) {\n                let row = Math.floor(Math.random() * gridSize);\n                let col = Math.floor(Math.random() * (gridSize - word.length));\n                if (grid[row].slice(col, col + word.length).every(cell => cell === \"\")) {\n                    for (let i = 0; i < word.length; i++) {\n                        grid[row][col + i] = word[i].toUpperCase();\n                    }\n                    placed = true;\n                }\n            }\n        }\n\n        function preencherGrade() {\n            palavrasSelecionadas.forEach(p => colocarPalavra(p.word.toUpperCase()));\n            for (let row = 0; row < gridSize; row++) {\n                for (let col = 0; col < gridSize; col++) {\n                    if (grid[row][col] === \"\") {\n                        grid[row][col] = String.fromCharCode(65 + Math.floor(Math.random() * 26));\n                    }\n                }\n            }\n        }\n\n        function renderizarGrade() {\n            const gridElement = document.getElementById(\"grid\");\n            gridElement.innerHTML = \"\";\n            grid.forEach((row, rowIndex) => {\n                row.forEach((letter, colIndex) => {\n                    const cell = document.createElement(\"div\");\n                    cell.classList.add(\"cell\");\n                    cell.textContent = letter;\n                    cell.dataset.row = rowIndex;\n                    cell.dataset.col = colIndex;\n                    cell.dataset.letter = letter;\n                    cell.addEventListener(\"click\", () => verificarLetra(cell));\n                    gridElement.appendChild(cell);\n                });\n            });\n        }\n\n        function mostrarPalavras() {\n            const listElement = document.getElementById(\"word-list\");\n            listElement.innerHTML = \"\";\n            palavrasSelecionadas.forEach(p => {\n                const li = document.createElement(\"li\");\n                li.innerHTML = `<span id=\"word-${p.word}\" class=\"word\">${p.word.toUpperCase()}<\/span><br>\n                                <span class=\"description\" id=\"desc-${p.word}\">${p.desc}<\/span>`;\n                listElement.appendChild(li);\n            });\n        }\n\n        function verificarLetra(cell) {\n            let found = false;\n\n            palavrasSelecionadas.forEach(p => {\n                let palavra = p.word.toUpperCase();\n                let letrasEncontradas = document.querySelectorAll(`.cell.found[data-letter=\"${cell.textContent}\"]`).length;\n                \n                if (grid[cell.dataset.row].join(\"\").includes(palavra) && letrasEncontradas < palavra.length) {\n                    cell.classList.add(\"found\");\n                    found = true;\n                }\n            });\n\n            if (found) verificarPalavraCompleta();\n        }\n\n        function verificarPalavraCompleta() {\n            palavrasSelecionadas.forEach(p => {\n                let palavra = p.word.toUpperCase();\n                let cells = document.querySelectorAll(\".cell.found\");\n                let palavraCompleta = palavra.split(\"\").every(letra => {\n                    return Array.from(cells).some(cell => cell.textContent === letra);\n                });\n\n                if (palavraCompleta && !document.getElementById(`desc-${p.word}`).style.display) {\n                    document.getElementById(`desc-${p.word}`).style.display = \"block\";\n                    document.getElementById(`word-${p.word}`).style.color = \"#13DC01\";\n                    palavrasEncontradas++;\n                }\n            });\n\n            if (palavrasEncontradas === palavrasSelecionadas.length) {\n                document.getElementById(\"message\").style.display = \"block\";\n                document.getElementById(\"coupon\").style.display = \"block\";\n            }\n        }\n\n        selecionarPalavras();\n        preencherGrade();\n        renderizarGrade();\n        mostrarPalavras();\n    <\/script>\n\n<\/body>\n<\/html>\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"elementor-element elementor-element-ff6ae6e e-flex e-con-boxed e-con e-parent\" data-id=\"ff6ae6e\" data-element_type=\"container\">\n\t\t\t\t\t<div class=\"e-con-inner\">\n\t\t\t\t<div class=\"elementor-element elementor-element-5b22e27 elementor-widget elementor-widget-html\" data-id=\"5b22e27\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<!DOCTYPE html>\n<html lang=\"pt-br\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Voltar para P\u00e1gina Inicial<\/title>\n    <style>\n        * {\n            box-sizing: border-box;\n            margin: 0;\n            padding: 0;\n        }\n\n        body {\n            font-family: 'Montserrat', sans-serif;\n            background-color: #fff;\n            color: #333;\n            text-align: center;\n            padding: 20px;\n        }\n\n        h1 {\n            color: #13DC01;\n            font-size: 24px;\n            margin-bottom: 20px;\n        }\n\n        .alert {\n            font-size: 18px;\n            color: #ff4d4d;\n            margin-top: 20px;\n        }\n\n        .home-btn-container {\n            margin-top: 30px;\n        }\n\n        .home-btn {\n            background-color: #13DC01;\n            color: #000;\n            font-size: 16px;\n            font-weight: bold;\n            padding: 15px;\n            border: none;\n            border-radius: 5px;\n            cursor: pointer;\n            transition: all 0.3s;\n            width: 100%;\n            font-family: 'Cerrad', sans-serif;\n            display: inline-block;\n            text-decoration: none;\n            text-align: center;\n        }\n\n        .home-btn:hover {\n            background-color: #0eb301;\n        }\n\n        \/* Responsividade para telas menores *\/\n        @media (max-width: 768px) {\n            body {\n                padding: 15px;\n            }\n            h1 {\n                font-size: 20px;\n            }\n            .home-btn {\n                font-size: 14px;\n                padding: 12px;\n            }\n        }\n\n        @media (max-width: 480px) {\n            h1 {\n                font-size: 18px;\n            }\n            .home-btn {\n                font-size: 14px;\n                padding: 12px;\n            }\n        }\n    <\/style>\n<\/head>\n<body>\n    <h1> Quer voltar para a p\u00e1gina inicial? <\/h1>\n    <p class=\"alert\">Se voc\u00ea quiser voltar para a p\u00e1gina inicial, clique no bot\u00e3o abaixo.<\/p>\n\n    <div class=\"home-btn-container\">\n        <a href=\"https:\/\/blog.primeiravendaonlinebr.com\/jogos\/\" class=\"home-btn\">Voltar para a P\u00e1gina Inicial<\/a>\n    <\/div>\n<\/body>