tainer.appendChild(link);\n      }\n    }\n  }\n\n  updateUI();\n\n  document.getElementById('coinIcon').addEventListener('click', () => {\n    const panel = document.getElementById('coinPanel');\n    panel.style.display = (panel.style.display === 'block') ? 'none' : 'block';\n  });\n\n  setInterval(() => {\n    timeOnPage++;\n    if (timeOnPage % 60 === 0) {\n      coins++;\n      localStorage.setItem('pvo_coins', coins);\n      animateCoin();\n      updateUI();\n    }\n  }, 1000);\n\n  function redeemBonus(cost, bonusId, url) {\n    if (unlocked[bonusId]) return;\n    if (coins >= cost) {\n      coins -= cost;\n      localStorage.setItem('pvo_coins', coins);\n      unlocked[bonusId] = url;\n      localStorage.setItem('pvo_unlocked', JSON.stringify(unlocked));\n      updateUI();\n    } else {\n      alert('\u26d4 Voc\u00ea ainda n\u00e3o tem moedas suficientes.');\n    }\n  }\n\n  function animateCoin() {\n    const coin = document.getElementById('coinSVG');\n    coin.classList.add('coin-bounce');\n    setTimeout(() => {\n      coin.classList.remove('coin-bounce');\n    }, 400);\n  }\n<\/script>\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t","protected":false},"excerpt":{"rendered":"<p>Curso Gratuito de Marketing Digital By @keninbusiness Aula 1 &#8211; Introdu\u00e7\u00e3o ao Curso Nesta aula, vamos te dar uma vis\u00e3o geral sobre o curso e o que voc\u00ea vai aprender. \u25c0 Aula Anterior Pr\u00f3xima Aula \u25b6 Aulas \u2630 Ver Aulas Aulas \ud83c\udf89 Curtiu o curso gratuito? N\u00e3o esquece de tirar um print e marcar a [&hellip;]<\/p>\n","protected":false},"author":1,"featured_media":0,"parent":0,"menu_order":0,"comment_status":"closed","ping_status":"closed","template":"elementor_header_footer","meta":{"_uag_custom_page_level_css":"","footnotes":""},"class_list":["post-4341","page","type-page","status-publish","hentry"],"uagb_featured_image_src":{"full":false,"thumbnail":false,"medium":false,"medium_large":fal