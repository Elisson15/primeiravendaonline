appendChild(title.cloneNode(true));\n    mobileList.appendChild(listMobile);\n  });\n}\n\nfunction changeLesson(moduloIndex, aulaIndex) {\n  currentModuleIndex = moduloIndex;\n  currentLessonIndex = aulaIndex;\n  const aula = courseData[moduloIndex].aulas[aulaIndex];\n\n  document.getElementById(\"videoPlayer\").src = aula.videoUrl;\n  document.getElementById(\"lessonTitle\").textContent = aula.title;\n  document.getElementById(\"lessonDescription\").innerHTML = aula.description;\n\n  renderSidebar();\n}\n\nfunction toggleMobileMenu() {\n  const menu = document.getElementById(\"mobileSidebar\");\n  menu.style.display = menu.style.display === \"block\" ? \"none\" : \"block\";\n}\n\ndocument.getElementById(\"prevLesson\").onclick = () => {\n  if (currentLessonIndex > 0) {\n    changeLesson(currentModuleIndex, currentLessonIndex - 1);\n  } else if (currentModuleIndex > 0) {\n    const prevModule = courseData[currentModuleIndex - 1];\n    changeLesson(currentModuleIndex - 1, prevModule.aulas.length - 1);\n  }\n};\n\ndocument.getElementById(\"nextLesson\").onclick = () => {\n  const currentModule = courseData[currentModuleIndex];\n  if (currentLessonIndex < currentModule.aulas.length - 1) {\n    changeLesson(currentModuleIndex, currentLessonIndex + 1);\n  } else if (currentModuleIndex < courseData.length - 1) {\n    changeLesson(currentModuleIndex + 1, 0);\n  }\n};\n\nrenderSidebar();\nchangeLesson(0, 0); \/\/ Aula inicial\n<\/script>\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-c1b689e elementor-widget elementor-widget-html\" data-id=\"c1b689e\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<div style=\"text-align: center; font-size: 11px; color: #666; margin-top: 40px; padding: 10px;\">\n  <p>\ud83c\udf89 Curtiu o curso gratuito?<\/p>\n  <p>N\u00e3o esquece de tirar um print e marcar a gente l\u00e1 no instagram <a href=\"https:\/\/www.instagram